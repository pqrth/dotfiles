# OS specific plugins
if [[ $CURRENT_OS == 'OS X' ]]; then
    antigen bundle osx
    antigen bundle brew-cask
elif [[ $CURRENT_OS == 'Linux' ]]; then
    if [[ $DISTRO == 'CentOS' ]]; then
    fi
elif [[ $CURRENT_OS == 'Cygwin' ]]; then
fi

# Antigen Theme
antigen theme bhilburn/powerlevel9k powerlevel9k

antigen apply
