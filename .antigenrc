    zsh-users/zsh-syntax-highlighting
    Tarrasch/zsh-autoenv
    sudo
    thefuck

    # SSH Plugins
    ssh-agent
    gko/ssh-connect

    # Git Plugins
    git
    gitignore

    # Java Plugins
    ant
    mvn

    # Ruby Plugins
    rbenv
    ruby
    gem
    bundler
    rails
    rake
    rake-fast

    # Node Plugins
    coffee
    node
    npm
    lukechilds/zsh-better-npm-completion

    # Python Plugins
    autopep8
    pep8
    pip
    pylint
    python
    pyenv

    # Docker
    docker
    docker-compose

    # Misc
    aws
